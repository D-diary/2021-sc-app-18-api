<%- include('../inc/head') %>
<div class="container">
  <h1 class="fa-2x">API KEY 발행</h1>
  <hr>
  <form action="/dev" method="post">
    <table class="table border-bottom">
      <tbody>
        <tr>
          <td class="title">아이디</td>
          <td class="content"><input type="text" name="userid" class="form-control"></td>
        </tr>
        <tr>
          <td class="title">도메인</td>
          <td class="content"><input type="text" name="domain" class="form-control"></td>
        </tr>
        <tr>
          <td class="title">API Key</td>
          <td class="content"><input type="text" name="apikey" class="form-control" readonly></td>
        </tr>
      </tbody>
    </table>
    <div class="btn-wrap text-center">
			<button class="btn btn-primary">API Key 생성/확인</button>
			<button class="btn btn-success">API 테스트 페이지</button>
		</div>
  </form>
</div> 
<%- include('../inc/foot') %> 